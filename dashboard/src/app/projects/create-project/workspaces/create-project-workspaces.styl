.create-project-workspace-list
  padding-left 30px
  box-shadow none

  .samples-list
    box-sizing content-box
    max-height 200px
    overflow-y auto
    box-shadow none

  .list-item
    outline none
    padding 10px
    cursor pointer
    min-height 40px

  .list-item-active
    background-color $primary-color

  .list-item-active .list-name
    color $white-color

  .list-empty
    font-size 12px
